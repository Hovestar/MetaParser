START		::= RUnion
RUnion		::= RIntersect {"|" RIntersect}
RIntersect	::= RConcat {"&" concat}
RConcat		::= RNeg {RNeg}
RNeg		::= "~" RNeg | ROptions
ROptions	::= atom {"*"|"+"|"?"}
atom		::= RNoString | REmptyString | RSingle | RAnyChar | "("RUnion")"
RNoString	::= "!"
REmptyString::= "#"
RSingle		::= ALPHANUM
RAnyChar	::= "."
